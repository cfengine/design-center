body common control {

               inputs => { "/var/cfengine/inputs/cfengine_stdlib.cf" };

                    }


bundle agent example {

  vars:

      "config[awesome]" string => "on";

      "config[cool]" string => "high";

  files:

      "/tmp/example.conf"

            edit_line => set_variable_values("example.config"),
               create => "true";
                     }

