body common control {
               inputs => { "/var/cfengine/inputs/libraries/cfengine_stdlib.cf" };
                    }

bundle agent example {

  files:

      "/tmp/data.txt"
            edit_line => change_dogs_to_cats;

                     }


bundle edit_line change_dogs_to_cats {

  replace_patterns:

      "dog"

         replace_with => value("cat");

                                     }
